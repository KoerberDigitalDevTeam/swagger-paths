{
  "name": "swagger-paths",
  "version": "1.0.0",
  "description": "Match paths from a swagger file",
  "main": "index.js",
  "scripts": {
    "test": "mocha 'test/**/*.test.js' || false",
    "lint": "eslint --cache '.eslintrc.js' '**/*js'",
    "lintfix": "eslint --cache '.eslintrc.js' '**/*js' --fix",
    "coverage": "nyc --reporter=text npm run test",
    "coverage-report": "nyc --reporter=html npm run test",
    "build": "npm run lint && npm run coverage"
  },
  "author": {
    "name": "Pier Fumagalli",
    "email": "pier.fumagalli@koerber.digital"
  },
  "homepage": "https://github.com/KoerberDigitalDevTeam/swagger-paths#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KoerberDigitalDevTeam/swagger-paths.git"
  },
  "bugs": {
    "url": "https://github.com/KoerberDigitalDevTeam/swagger-paths/issues"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^5.4.0",
    "eslint-config-google": "^0.9.1",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1"
  },
  "nyc": {
    "watermarks": {
      "lines": [ 95, 99 ],
      "functions": [ 95, 99 ],
      "branches": [ 95, 99 ],
      "statements": [ 95, 99 ]
    }
  }
}
